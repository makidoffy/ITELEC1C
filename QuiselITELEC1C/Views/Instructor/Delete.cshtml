@model Instructor;
<h1>Delete</h1>

<form asp-action="Delete" asp-controller="Instructor" method="post">

    <div class="form-group">
        <label asp-for="FirstName">FirstName</label>
        <input asp-for="FirstName" class="form-control" /  hidden >
    </div>

    <div class="form-group">
        <label asp-for="LastName">LastName</label>
        <input asp-for="LastName" class="form-control" / readonly>
    </div>

    <div class="form-group">
        <label asp-for="Rank">Rank</label>
        <input asp-for="Rank" class="form-control" / readonly>
    </div>

    <div class="form-group">
        <label asp-for="Status">Status</label>
        <input asp-for="Status" class="form-control" / readonly>
    </div>

    <div class="form-group">
        <label asp-for="HiringDate">HiringDate</label>
        <input asp-for="HiringDate" class="form-control" / readonly>
    </div>

    <button type="submit" class="btn btn-primary">Confirm</button>
    <button type="button" onclick="history.back()" class="btn btn-primary-secondary">Cancel</button>


</form>
