@model Student;
<h1>Delete</h1>

<form asp-action="Delete" asp-controller="Student" method="post">

    <div class="form-group">
        <label asp-for="FirstName">FirstName</label>
        <input asp-for="FirstName" class="form-control" /  hidden >
    </div>

    <div class="form-group">
        <label asp-for="LastName">LastName</label>
        <input asp-for="LastName" class="form-control" / readonly>
    </div>

    <div class="form-group">
        <label asp-for="Course">Course</label>
        <input asp-for="Course" class="form-control" / readonly>
    </div>

    <div class="form-group">
        <label asp-for="DateEnrolled">DateEnrolled</label>
        <input asp-for="DateEnrolled" class="form-control" / readonly>
    </div>

    <div class="form-group">
        <label asp-for="Email">Email</label>
        <input asp-for="Email" class="form-control" / readonly >
    </div>

    <button type="submit" class="btn btn-primary">Confirm</button>
    <button type="button" onclick="history.back()" class="btn btn-primary-secondary">Cancel</button>


</form>
