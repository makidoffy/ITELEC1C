@model Instructor;
<h1>Create a new Instructor</h1>

<form asp-action="AddInstructor" asp-controller="Instructor" method="post" enctype="multipart/form-data">
    <div class="form-group">
        <label asp-for="StudentProfilePhoto"></label>
        <input type="file" class="form-control" asp-for="StudentProfilePhoto"/>
    </div>
    <div class="form-group">
        <label asp-for="FirstName"></label>
        <input asp-for="FirstName" class="form-control" />
    </div>

    <div class="form-group">
        <label asp-for="LastName"></label>
        <input asp-for="LastName" class="form-control" />
    </div>

    <div class="form-group">
        <label asp-for="Rank"></label>
        <input asp-for="Rank" class="form-control" />
    </div>

    <div class="form-group">
        <label asp-for="Status"></label>
        <input asp-for="Status" class="form-control" />
    </div>

    <div class="form-group">
        <label asp-for="HiringDate"></label>
        <input asp-for="HiringDate" class="form-control" />
    </div>

    <div class="form-group">
        <label asp-for="PhoneNumber"></label>
        <input asp-for="PhoneNumber" class="form-control" />
    </div>

    <button type="submit" class="btn btn-primary"> Save</button>


</form>
